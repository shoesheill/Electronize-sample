{
  "executable": "WebApplication5",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "WebApplication5",
  "author": "",
  "singleInstance": false,
  "environment": "Development",
  "build": {
    "appId": "com.WebApplication5.app",
    "productName": "WebApplication5",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./additional-files",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [
          "**/*"
        ]
      },
      "**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    }
  }
}